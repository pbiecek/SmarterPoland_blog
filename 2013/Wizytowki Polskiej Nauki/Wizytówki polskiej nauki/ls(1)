var PowiekszanieTekstu=new Class({Implements:[Options],options:{'selektor':'body','selektorKonteneraLinkow':'#text-sizing','klasyLinkow':['czcionkaZmniejsz','text-size-normal','text-size-medium','text-size-big','czcionkaPowieksz']},initialize:function(options){this.setOptions(options);this.element=document.id($$(this.options.selektor)[0]);this.klasaCss=this.options.klasyLinkow[1];this.kontenerLinkow=$$(this.options.selektorKonteneraLinkow)[0];if(this.kontenerLinkow){this.dodajZdarzenia();this.ustawCzcionke();}},dodajZdarzenia:function(){this.kontenerLinkow.addEvent('click',function(e){if(e){e.preventDefault();}
var linkClass=e.target.get('class');if(this.options.klasyLinkow.contains(linkClass)){this.ustawCzcionke(linkClass);}
switch(e.target.className)
{case'text-size-big':this.ustawCzcionke('czcionkaMax');document.body.removeClass('czcionkaMid');document.body.addClass('czcionkaMax');break;case'text-size-medium':this.ustawCzcionke('czcionkaMid');document.body.removeClass('czcionkaMax');document.body.addClass('czcionkaMid');break;case'text-size-normal':this.ustawCzcionke('');document.body.removeClass('czcionkaMax');document.body.removeClass('czcionkaMid');break;}}.bind(this));},ustawCzcionke:function(klasaCss){if(!klasaCss){klasaCss=Cookie.read('czcionka')?Cookie.read('czcionka'):this.options.klasyLinkow[1];}else{this.reset();}
this.klasaCss=klasaCss;this.element.addClass(this.klasaCss);Cookie.write('czcionka',this.klasaCss,{duration:14});},reset:function(){this.element.removeClass(this.klasaCss);}});function obsluzNoClick(){var nieklikalke=function(e){e.preventDefault();}
$$('.noClick').each(function(el){el.addEvent('click',nieklikalke);el.addEvent('contextmenu',nieklikalke);});}
window.addEvent('domready',function(){powiekszanieTekstu();obsluzNoClick();});function menuExpander(){var submenu=$$('#sidebar .navigation li > ul');submenu.hide();submenu.getSiblings('a').each(function(el){var linkParent=el.getParent();linkParent.addClass('has-subs');var linkExpand=new Element('a',{'class':'expand','href':'#'});linkParent.grab(linkExpand);var linkExpandHelper=new Element('span',{'class':'helper'});linkExpand.adopt(linkExpandHelper);linkParent.adopt(el.getSiblings('ul'));var linkText=el.getChildren('span')[0].get('html');linkExpandHelper.set('text',JS.lang.rozwin+' "'+linkText+'"');});var expander=$$('a.expand');expander.each(function(el){var rozwijaque=function(event){this.getParent().toggleClass('selected');this.getNext('ul').toggle();if(this.getParent().hasClass('selected')){this.getChildren('.helper')[0].set('text',JS.lang.zwin);}
else{this.getChildren('.helper')[0].set('text',JS.lang.rozwin);}
event.preventDefault();}
el.addEvent('click',rozwijaque);el.addEvent('keydown',function(event){if(event.key=='enter'){rozwijaque;}});});$$('#sidebar li.active').each(function(el){el.getParent('ul').show();if(el.getElements('li.active')[0]){el.addClass('selected');}});$$('#sidebar li.has-subs > a.noClick').each(function(el){el.addClass('noClickExpander');el.removeClass('noClick');var noClickExpanderArrow=new Element('span',{'class':'expand'});noClickExpanderArrow.inject(el);var noClickExpanderHelper=new Element('span',{'class':'helper'});noClickExpanderHelper.inject(noClickExpanderArrow);var expanderToDestroy=el.getSiblings('.expand')[0];expanderToDestroy.getParent().removeChild(expanderToDestroy);if(el.getParent().hasClass('selected')){el.getElements('.helper')[0].set('text',JS.lang.zwin);}
else{el.getElements('.helper')[0].set('text',JS.lang.rozwin);}
el.addEvent('click',function(){this.getParent().toggleClass('selected');this.getNext('ul').toggle();if(this.getParent().hasClass('selected')){this.getElements('.helper')[0].set('text',JS.lang.zwin);}
else{this.getElements('.helper')[0].set('text',JS.lang.rozwin);}});})}
window.addEvent('domready',function(){menuExpander();});window.addEvent('domready',function(){var advanced_collapser=new Element('a');advanced_collapser.addClass('collapser collapser-down');var search_results_form=$('search-results-form');if(search_results_form!=null){var advanced_collapserTitle=search_results_form.get('data-advanced-title');var advanced_collapserText=search_results_form.get('data-advanced-text');advanced_collapser.set({href:'#',id:'collapser-search',text:advanced_collapserText,title:advanced_collapserTitle});var p_advanced_collapser=new Element('div',{'class':'p-advanced-collpaser'});p_advanced_collapser.adopt(advanced_collapser);$$('#search-results-form > div:first-child').adopt(p_advanced_collapser);var submit_button=$$('#search-results-form input[type="submit"]')[0];var search_form=$$('#content form[name="wyszukiwarka"]')[0];var search_form_action=search_form.get('action');search_form_action_hash=search_form_action+'#wyszukiwanie-zaawansowane';search_form.addEvent('submit',function(event){if($('collapser-search-box').getStyle('display')!='none'){search_form.setAttribute('action',search_form_action_hash);}
else{search_form.setAttribute('action',search_form_action);}
event.preventDefault();search_form.submit();});}});window.addEvent('domready',function(){$$('.collapser').each(function(el){var collapser_id=el.get('id');var collapser_box=$(collapser_id+'-box');if(el.hasClass('collapser-down')){collapser_box.hide();}
el.addEvent('click',function(event){el.toggleClass('collapser-down');el.toggleClass('collapser-up');collapser_box.toggle();event.preventDefault();});});var collapser_search=$('collapser-search');var collapser_search_box=$('collapser-search-box');if(collapser_search!=null){var the_url=new URI(location);var url_anchor=the_url.get('fragment');if(url_anchor=='wyszukiwanie-zaawansowane'){collapser_search.removeClass('collapser-down').addClass('collapser-up');collapser_search_box.show();}}});window.addEvent('domready',function(){$$('.table-expand').each(function(el){var table_header=el.getChildren('.table-title');var table_header_text=table_header.get('text');table_header.set('html',null);var linkHref=new Element('a',{title:JS.lang.rozwin_liste,"class":"table-collapser collapser-up",href:'#',text:table_header_text});table_header.grab(linkHref);var collapser_target=linkHref.getParent().getSiblings();linkHref.addEvent('click',function(ev){collapser_target.each(function(el){el.toggle();});linkHref.toggleClass('collapser-down');linkHref.toggleClass('collapser-up');ev.preventDefault();});if(linkHref.getParent().getProperty('data-collapsed')==='true'){linkHref.removeClass('collapser-up');linkHref.addClass('collapser-down');collapser_target[0].hide();}});});window.addEvent('keydown',function(event){if(event.key=='tab'){$$('#go-to-the-content a')[0].addClass('show');}});if(Browser.ie&&Browser.version=='10'){document.documentElement.set('id','ie10');}
(function(){var len,i,links,href,host1,host2;links=document.links;len=links.length;if(/www/.test(window.location.hostname)){host1=window.location.hostname;host2=window.location.hostname.replace('www.','');}
else{host1='www.'+window.location.hostname;host2=window.location.hostname;}
for(i=0;i<len;i++){href=links[i].href;if(!/http/.test(href)){continue;}
if(!href.match('/'+host1+'|'+host2+'/')){links[i].set('html',links[i].get('html')+'<span class="helper">Przeniesienie do serwisu zewntrznego</span>');links[i].set('title',links[i].get('title')+' (Przeniesienie do serwisu zewntrznego)');}}})();window.addEvent('domready',function(){$$('.content-skiplink').each(function(el){var hash=el.get('href').substr(1);el.addEvent('click',function(){$(hash).setAttribute('tabIndex',0);});});});